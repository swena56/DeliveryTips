<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.deliverytips.fragments.Import">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="CSV Import" />

    <Button
        android:id="@+id/buttonGetCSV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Get CSV"/>

    <Button
        android:id="@+id/buttonProcessCSV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Process CSV"/>

    <Button
        android:id="@+id/buttonImportDataset"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        android:text="Import Dataset"/>

    <TextView
        android:id="@+id/textViewOutput"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="" />
    </LinearLayout>
</FrameLayout>
